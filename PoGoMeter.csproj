    <UserSecretsId>d069722e-7021-4c85-a616-058f22ef3fbc</UserSecretsId>
    <LangVersion>latest</LangVersion>
    <DefaultItemExcludes>$(DefaultItemExcludes);TelegramSkeleton\**\*</DefaultItemExcludes>
